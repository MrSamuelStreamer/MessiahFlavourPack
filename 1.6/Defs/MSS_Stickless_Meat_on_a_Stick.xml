<Defs>

  <!-- Stickless Meat -->

  <ThingDef ParentName="MealCookedIngredientless">
    <defName>MSS_SticklessMeat</defName>
    <label>Stickless Meat on a Stick</label>
    <description>Meat skewered onto a stick, but then the stick is removed from the meat, so you can eat the meat without the inherent risk of splinters.</description>
    <graphicData>
      <texPath>Things/Item/Meal/SticklessMeat</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>8</MarketValue>
      <WorkToMake>190</WorkToMake>
      <Nutrition>0.9</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealSimple</preferability>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
  </ThingDef>

